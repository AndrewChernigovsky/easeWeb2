nav(class=menuClasses[0])
    div(class=menuClasses[1])
        include logo
    div(class=menuClasses[2])
        each ul in menuClasses
            if ul === navUlClasses
                ul(class=ul)
                    li(class=menuClasses[4])
                        if mainPage
                            a(class=menuClasses[5] href="#service")= 'Услуги'
                        else
                            a(class=menuClasses[5] href="../index.html#service")= 'Услуги'
                    li(class=menuClasses[4])
                        if mainPage
                            a(class=menuClasses[5] href="#contacts" id='contactsLink')= 'Контакты'
                        else
                            a(class=menuClasses[5] href="../index.html#contacts" id='contactsLink')= 'Контакты'
                    li(class=menuClasses[4])
                        a(class=menuClasses[5] href=`tel:${footerMain.tel}`)= footerMain.tel

        button(class=menuButton, id=menubtn) Заказать звонок